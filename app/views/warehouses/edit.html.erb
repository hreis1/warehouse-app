<% if @warehouse.errors.any? %>
    <div>
        <p>Verifique os seguintes erros:</p>
        <ul>
            <% @warehouse.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
        </ul>
    </div>
<% end %>

<%= form_with model: @warehouse do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
  
    <%= f.label :code %>
    <%= f.text_field :code %>
  
    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :area %>
    <%= f.number_field :area %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :cep %>
    <%= f.text_field :cep %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit %>
<% end %>